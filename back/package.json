{
  "name": "nodejs-express-sequelize-mysql",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL",
  "scripts": {
    "start": "NODE_PATH=./src NODE_ENV=development nodemon -x \"node -r ts-node/register --preserve-symlinks src/server.ts\"",
    "build": "swc src --copy-files -d build",
    "test": "NODE_PATH=./src mocha -r ts-node/register $(find ./src/app -name '*.test.js') --exit",
    "test-watch": "NODE_PATH=./src mocha -r ts-node/register $(find ./src/app -name '*.test.js') --watch",
    "migrate:up": "node ./src/app/scripts/run-migrations.js up",
    "migrate:down": "node ./src/app/scripts/run-migrations.js down",
    "templates": "NODE_ENV=development nodemon --ignore public ./src/app/scripts/try-template.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "author": "StrongNode Team",
  "license": "ISC",
  "dependencies": {
    "@levminer/speakeasy": "^1.4.0",
    "@types/cors": "2.8.4",
    "@types/express": "^4.17.13",
    "ansi-regex": "^5.0.1",
    "aws-sdk": "^2.984.0",
    "axios": "^0.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "coingecko-api": "^1.0.10",
    "cors": "2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.2",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mocha": "^9.2.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "mysql2": "^2.0.2",
    "node-cron": "^3.0.0",
    "path": "^0.12.7",
    "qrcode": "^1.4.4",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^6.3.0",
    "ts-node": "^10.7.0",
    "umzug": "^3.0.0",
    "validator": "13.7.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.157",
    "@types/bluebird": "^3.5.36",
    "@types/connect-livereload": "^0.6.0",
    "@types/livereload": "^0.9.2",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.22",
    "@types/validator": "^13.7.1",
    "@types/websocket": "^1.0.5",
    "@types/ws": "^8.5.3",
    "connect-livereload": "^0.6.1",
    "i": "^0.3.7",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.2"
  }
}
