    "axios": "^0.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mocha": "^9.2.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
