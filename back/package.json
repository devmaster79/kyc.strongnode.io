{
  "name": "nodejs-express-sequelize-mysql",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL",
  "scripts": {
    "start": "NODE_ENV=development ts-node src/server.ts",
    "build": "tsc --build tsconfig.json",
    "test": "mocha -r ts-node/register ./src/app/**/*.test.js",
    "test-watch": "mocha -r ts-node/register ./src/app/**/*.test.js --watch",
    "migrate:up": "node ./src/app/scripts/run-migrations.js up",
    "migrate:down": "node ./src/app/scripts/run-migrations.js down"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
